        }

            .VATTEMP .cusname td, .comname td {
                padding: 0px 0px 5px 6px;
            }

        tfoot .noline td {
            padding-left: 5px !important;
        }

        .VATTEMP .input-code {
            border: 1px solid #000;
            color: #000;
            float: left;
            font-weight: normal;
            text-align: center;
            width: 18px;
            height: 14px;
        }

        .header-note p i {
            font-size: 13px;
        }

        .VATTEMP .dongcuoi tr:last-child {
            border-bottom: rgb(7, 74, 142) solid thin !important;
        }

        .VATTEMP div label.fl-l,
        div label {
            margin-right: 5px;
            margin-top: 0px;
        }

        .VATTEMP .input-name,
        .input-date {
            border: 0;
            border-bottom: 1px dotted #000;
        }

        .VATTEMP .statistics {
            clear: both;
            margin-right: 0;
            padding-top: 2px;
        }

        .nenhd {
            position: relative;
        }

        .nenhd_bg {
            background: url('data:image/png;base64,') no-repeat;
            top: 298px;
            left: 0;
            /* right: 0; */
            background-size: 28%;
            width: 100%;
            background-repeat: no-repeat;
            height: 340px;
            margin: 0 auto;
            background-position: center center;
            position: absolute;
            /* z-index: -1; */
            opacity: 0.15;
        }

        .VATTEMP .pagecurrent {
        }

        .dulieu {
            color: #000;
            text-align: right;
        }

        .VATTEMP .statistics table {
            background-position: bottom;
            border: 0 none;
            font-size: 14px;
            margin: 0;
            position: relative;
            z-index: 2;
            width: 100%;
            table-layout: fixed;
        }

        table {
            border-collapse: collapse;
            font-family: 'Time new roman';
            font-size: 14px;
        }

        .VATTEMP .statistics table th.h1 i {
            font-size: 13px;
        }

        .VATTEMP .statistics table th.h1 {
            font-size: 15px;
            text-transform: none;
            color: rgb(7, 74, 142) !important;
            color: #000;
            padding: 2px;
            font-weight: normal;
            border-right: 1px solid rgb(7, 74, 142);
            border-left: 1px solid rgb(7, 74, 142);
            border-top: 1px solid rgb(7, 74, 142);
            background-color: rgba(255, 255, 255, 0);
        }

        .VATTEMP .statistics table th.h2 {
            font-size: 14px;
            text-transform: none;
            color: #000;
            font-weight: normal;
            border-bottom: #000 solid thin;
            border-right: 1px solid rgb(7, 74, 142);
            border-left: 1px solid rgb(7, 74, 142);
            border-top: 1px solid #rgb(7, 74, 142);
            background-color: rgba(255, 255, 255, 0);
        }

        .VATTEMP .statistics table td.stt {
            text-align: center;
        }

        .VATTEMP .statistics table td.stt2 {
            text-align: center;
            color: #000;
        }

        .VATTEMP .statistics table .back td {
            color: #000;
            font-family: 'Time new roman';
            font-size: 15px;
        }

        .VATTEMP .statistics table .noline td {
            border-bottom: none;
            border-right: 1px solid rgb(7, 74, 142);
            border-left: 1px solid rgb(7, 74, 142);
            border-top: none;
        }

            .VATTEMP .statistics table .noline td:first-child {
            }

        .date_invoice i {
            font-size: 13px;
        }

        .VATTEMP .statistics table td {
            border-bottom: none;
            border-right: none;
            border-left: none;
            padding: 2px;
            word-wrap: break-word;
            overflow-wrap: break-word;
        }

        .VATTEMP .statistics tr td.back-bold {
            font-size: 14px;
            border-bottom: none;
            color: #000;
        }

        .VATTEMP .statistics table .back-bold {
            padding-right: 5px;
            text-align: right;
        }

        .VATTEMP .statistics tr td.back-bold2 {
            font-size: 14px;
            border-bottom: none;
            color: #000;
        }

        p.input-txt {
            color: #000;
            line-height: 16px;
            text-align: left;
            border-bottom: 1px dotted rgba(0, 0, 0, 0.5);
            font-family: 'Time new roman';
        }

        .clsCol p {
            padding-top: 4px;
            font-size: 14px;
        }

        .col-title {
            width: 1%;
            white-space: nowrap;
        }

            .col-title p i {
                font-size: 13px;
            }

            .col-title p {
                color: rgb(7, 74, 142) !important;
            }

        .clsCol {
            display: table-cell;
        }

        .clsTable {
            width: 100%;
        }

        .clsTable {
            clear: both;
        }

        .clsCol {
            display: table-cell;
        }

        p {
            margin: 0px 0 0px 0;
        }

        .VATTEMP .statistics table .back-bold2 {
            padding-left: 5px;
            text-align: left;
        }

        .VATTEMP .statistics tr.bg-pink td {
            font-size: 14px;
            text-align: right;
            color: #000;
            background: none repeat scroll 0 0 #fedccc;
        }

        .VATTEMP .payment,
        .date {
            margin: 0px 0;
            text-align: center;
            width: 35%;
        }

        .VATTEMP .payment {
            float: left;
        }

            .VATTEMP .payment p,
            .date p {
                margin: 0;
            }

        .VATTEMP .date {
            float: right;
            height: 120px;
        }

        .VATTEMP .input-date {
            width: 40px;
        }

        .VATTEMP .input-name,
        .back-bold,
        .input-date {
            color: #000;
            font-family: 'Time new roman';
            font-size: 14px;
        }

        .footer_invoice .clsCol p {
            font-size: 14px;
        }

        .fl-l {
            float: left;
            font-family: 'Time new roman';
            font-size: 14px;
        }

        .comname .col-txt p {
            color: rgb(7, 74, 142) !important;
        }

        .bgimg {
            border: 1px solid #000;
            cursor: pointer;
            width: 250px;
            height: 60px;
        }

            .bgimg p {
                color: #EB363A;
                padding-left: 10px;
                text-align: left;
            }

        .bold {
            font-weight: bold !important;
        }

        p {
            font-family: 'Time new roman';
            font-size: 14px;
        }

        .VATTEMP .header .number {
            color: #333;
            font-family: "Times New Roman",Times,serif;
            font-size: 150%;
        }

        .item {
            color: #000;
        }
    </style>

</head>
<body>
    <div id="printView" style="background-color: rgba(255, 255, 255, 0);;  margin-top: -3px;    width: 790px;&#xD;&#xA;    margin: auto;">
        <div class="VATTEMP" style="background-color: rgba(255, 255, 255, 0);">
            <div id="SolutionVNPT" style="margin-bottom:5px;margin-top:0px;font-family: 'Time New Roman';font-size:9px;border-bottom:1px dashed rgb(7, 74, 142); text-align: center;background-color: rgba(255, 255, 255, 0);;">
                <label style="font-family: 'Time New Roman';font-size:13px;color: rgb(7, 74, 142);">
                    Đơn vị cung cấp giải pháp hóa đơn điện tử: Tổng công ty dịch vụ viễn thông - VNPT Vinaphone, MST:0106869738, Điện thoại:18001260
                </label>
            </div><div id="quantitypages" style="padding-left:0px;">
                <div class="pagecurrent" id="1">
                    <table width="100%">
                        <tr>
                            <td style="width:24%;text-align: center"><div id="logo" /></td>
                            <td style="vertical-align: top;">
                                <table cellpadding="0" cellspacing="0" border="0" style="float: left; width:350px">
                                    <tbody>
                                        <tr>
                                            <td style="text-align: center;padding-left: 0%;">
                                                <p class="name-upcase" style="font-weight: bold;color:rgb(7, 74, 142);    line-height: 23px; margin-top: 10px; margin-bottom: 0px;;font-size:24px; text-transform: uppercase;">HÓA ĐƠN GIÁ TRỊ GIA TĂNG <br /><i style="font-size:19px; font-weight:normal">(VAT INVOICE)</i></p><p style=" font-size 15px;margin-top 5px; color:rgb(7, 74, 142) !important " class="date_invoice">
                                                    Ngày <i /><label style="color:#000"></label>
                                                    tháng <i /><label style="color:#000"></label>
                                                    năm <i /><label style="color:#000"></label>
                                                </p>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table><table border="0" style="width: 229px;">
                                    <tbody>
                                        <tr>
                                            <td class="header">
                                                <div class="header-note">
                                                    <p style="font-size:15px;    color: rgb(7, 74, 142) !important;">
                                                        Mẫu số <i>(Form)</i>:
                                                        <b>01GTKT0/001</b>
                                                    </p><p style="font-size:15px;margin-top: 5px;width: 100%;    color: rgb(7, 74, 142)  !important;">
                                                        Ký hiệu <i>(Serial)</i>:
                                                        <b>XX/XXX</b>
                                                    </p><p style="margin-top:0px;font-size:15px;      line-height: 30px;  color: rgb(7, 74, 142)  !important;">
                                                        Số <i>(No)</i>:
                                                        <b style="margin-top:0px;font-size:16px;"><span class="number" style="color:red !important">XXXXXXX</span></b>
                                                    </p>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                    </table><table width="100%" style="margin-left: 0px;margin-bottom:6px;&#xD;&#xA;    border-bottom: rgb(7, 74, 142) solid thin;" class="comname">
                        <tbody>
                            <tr>
                                <td colspan="3">
                                    <div class="clsTable">
                                        <div class="clsCol col-title" style="width:205px"><p style="font-family: 'Time new roman';">Đơn vị bán hàng <i>(Company name)</i></p></div><div class="clsCol col-txt">
                                            <p class="">
                                                :
                                                <b style="font-weight:bold; font-size:16px;">@Model.thongtin.tencn</b>
                                            </p>
                                        </div>
                                    </div><div class="clsTable">
                                        <div class="clsCol col-title" style="width:205px"><p style="font-family: 'Time new roman';">Mã số thuế <i>(Tax code)</i></p></div><div class="clsCol col-txt">
                                            <p class="">
                                                :
                                                <b>@Model.thongtin.mst</b>
                                            </p>
                                        </div>
                                    </div><div class="clsTable">
                                        <div class="clsCol col-title" style="width:205px"><p style="font-family: 'Time new roman';"> Địa chỉ <i>(Address)</i></p></div><div class="clsCol col-txt">
                                            <p class="">
                                                :
                                                @Model.thongtin.diachi
                                            </p>
                                        </div>
                                    </div><div class="clsTable">
                                        <div class="clsCol col-title" style="width:205px"><p style="font-family: 'Time new roman';">Điện thoại <i>(Tel)</i></p></div><div class="clsCol col-txt">
                                            <p class="">
                                                :
                                                @Model.thongtin.dienthoai
                                            </p>
                                        </div>
                                    </div><div class="clsTable">
                                        <div class="clsCol col-title" style="width:205px"><p style="font-family: 'Time new roman';">Số tài khoản <i>(Bank Account)</i></p></div><div class="clsCol col-txt">
                                            <p class="">
                                                :
                                                @Model.thongtin.taikhoannh
                                            </p>
                                        </div>
                                    </div><div class="clsTable">
                                        <div class="clsCol col-title" style="width:205px"><p style="font-family: 'Time new roman';">Ngân hàng<i>(Bank)</i></p></div><div class="clsCol col-txt">
                                            <p class="">
                                                :
                                                @Model.thongtin.tennh
                                            </p>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <table style="margin-top: 5px;   " class="cusname">
                        <tr><td><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';">Họ tên người mua hàng <i>(Customer's name)</i>:</p></div><div class="clsCol col-txt"><p class="input-txt" style="">@Model.Invoice.Buyer</p></div></div><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';">Tên đơn vị <i>(Company name)</i>: </p></div><div class="clsCol col-txt"><p class="input-txt" style="">@Model.Invoice.CusName</p></div></div><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';">Mã số thuế <i>(Tax code)</i>:</p></div><div class="clsCol col-txt"><p class="input-txt" style="">@Model.Invoice.CusTaxCode</p></div></div><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';">Địa chỉ <i>(Address)</i>: </p></div><div class="clsCol col-txt"><p class="input-txt" style="">@Model.Invoice.CusAddress</p></div></div><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';">Hình thức thanh toán <i>(Payment method)</i>:</p></div><div class="clsCol col-txt"><p class="input-txt" style="">@Model.Invoice.PaymentMethod</p></div></div><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';">Số tài khoản <i>(Bank Account)</i>: </p></div><div class="clsCol col-txt"><p class="input-txt" style="">@Model.Invoice.CusBankNo</p></div></div><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';">Ngân hàng <i>(Bank)</i>: </p></div><div class="clsCol col-txt"><p class="input-txt" style="">@Model.Invoice.CusBankName</p></div></div></td></tr>
                    </table>
                    @if (@Model.Invoice.Products != null)
                    {
                        <div class="statistics">
                            <div class="nenhd">
                                <table width="100%" class="dongcuoi" cellpadding="0" cellspacing="0" border="1" style="border-bottom: 0px solid">
                                    <tr style="border-bottom: #36426F 1px solid" height="23px">
                                        <th width="30px" class="h1 bold;" style="text-align:center;">
                                            STT
                                            <br /><i>(No.)</i>
                                        </th>
                                        <th width="210px" class="h1 bold;" style="text-align:center;">
                                            Tên hàng hóa, dịch vụ
                                            <br /><i>(Product, services )</i>
                                        </th>
                                        <th width="50px" class="h1 bold; " style="text-align:center;">
                                            Mã hàng <br /><i>(Code)</i>
                                        </th>
                                        <th width="55px" class="h1 bold;" style="text-align:center;">
                                            Đơn vị tính <br /><i>(Unit)</i>
                                        </th>
                                        <th width="65px" class="h1 bold; " style="text-align:center;">
                                            Số lượng<br /><i>(Quantity)</i>
                                        </th>
                                        <th width="80px" class="h1 bold; " style="text-align:center;">
                                            Đơn giá<br /><i>(Unit price)</i>
                                        </th>
                                        <th width="105px" class="h1 bold; " style="text-align:center;">
                                            Thành tiền<br /><i>(Amount)</i>
                                        </th>
                                        <th width="30px" class="h1 bold; " style="text-align:center;">
                                            CK <br /><i>(%)</i>
                                        </th>
                                    </tr>
                                    <tr style="border-bottom: #36426F 1px solid"><th class="h1">0</th><th class="h1">1</th><th class="h1">2</th><th class="h1">3</th><th class="h1">4</th><th class="h1">5</th><th class="h1">6 = 4 x 5</th><th class="h1">7</th></tr>
                                    @{ int count = 1;
                                        foreach (var i in Model.Invoice.Products.Product)
                                        {
                                            <tr class="noline back" style="border-bottom:#36426F dotted 1px ">
                                                <td class="stt" height="23px" style="text-align:center;">@count</td>
                                                <td class="back-bold2" height="23px" style="white-space: pre-wrap;">@Html.Raw(i.ProdName)</td>
                                                <td class="back-bold2" height="23px" style="text-align: center;">@Html.Raw(i.Code)</td>
                                                <td class="back-bold2" height="23px" style="text-align: center; white-space: pre-wrap;">@Html.Raw(i.ProdUnit)</td>
                                                <td class="back-bold" height="23px" style="text-align: right;">@i.ProdQuantity.ToString("N0", ViewBag.CurrentNumberFormat)</td>
                                                <td class="back-bold" height="23px">@i.ProdPrice.ToString("#,##0.####", ViewBag.CurrentNumberFormat)</td>
                                                <td class="back-bold" height="23px">@i.Total.ToString("N0", ViewBag.CurrentNumberFormat)</td>
                                                <td class="back-bold" height="23px">@i.DiscountAmount.ToString("0.##", ViewBag.CurrentNumberFormat)</td>
                                            </tr>
                                            count = count + 1;
                                        }
                                        if (Model.Invoice.Products.Product.Last().Extra2 != "" && Model.Invoice.Products.Product.Last().Extra2 != null)
                                        {
                                            <tr class="noline back" style="border-top:#000 solid 1px "><td style="border-bottom:none" height="23px" width="24px"></td><td colspan="6">@Model.Invoice.Products.Product.Last().Extra2</td><td style="border-bottom:none" height="23px" width="125px"></td></tr>
                                        }
                                    }
                                    @for (int i = 9; i > (9 - Model.Invoice.Products.Product.Count); i--)
                                    {
                                        <tr class="noline back" style="border-top:#000 solid 1px "><td style="border-bottom:none" height="23px" width="24px"></td><td style="border-bottom:none" height="23px" width="120px"></td><td style="border-bottom:none" height="23px" width="41px"></td><td style="border-bottom:none" height="23px" width="45px"></td><td style="border-bottom:none" height="23px" width="79px"></td><td style="border-bottom:none" height="23px" width="125px"></td><td style="border-bottom:none" height="23px" width="79px"></td><td style="border-bottom:none" height="23px" width="125px"></td></tr>
                                    }
                                    <tfoot>
                                        <tr class="noline back" style="border-top: 1px solid #36426F; border-bottom: 0px;"><td class="back-bold" colspan="7" height="20px" width="136px" style="padding-right: 3px;border-right:none;text-align:left;   color: rgb(7, 74, 142) !important;"><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';"> Cộng tiền hàng <i>(Total amount)</i>:</p></div><div class="clsCol col-txt"><p class="dulieu">@Model.Invoice.Total.ToString("N0", ViewBag.CurrentNumberFormat)</p></div></div></td><td /></tr>
                                        <tr class="noline back" style="border-top: 1px solid #36426F; border-bottom: 0px;"><td class="back-bold" colspan="7" height="20px" width="136px" style="padding-right: 3px;border-right:none;text-align:left;   color: rgb(7, 74, 142) !important;"><div class="clsTable"><div class="clsCol col-title"><p style="font-family: 'Time new roman';"> Chiết khấu <i>(Discount)</i>:</p></div><div class="clsCol col-txt"><p class="dulieu">@Model.Invoice.DiscountAmount.ToString("N0", ViewBag.CurrentNumberFormat)</p></div></div></td><td /></tr>
                                        <tr class="noline back" style="border-top: #36426F solid thin; border-bottom:0px">
                                            <td class="back-bold" colspan="7" height="20px" width="136px" style="border-right:none;text-align:left;">
                                                <div class="clsTable">
                                                    <div class="clsCol col-title">
                                                        <p style="font-family: 'Time new roman';">
                                                            Thuế suất GTGT <i>(VAT rate)</i> :
                                                        </p>
                                                    </div><div class="clsCol col-txt" style="width:10%">
                                                        <p class="input-txt dulieu" style="text-align:right; border-bottom: 0px;">
                                                            @Model.Invoice.VATRate
                                                            %
                                                        </p>
                                                    </div><div class="clsCol col-title">
                                                        <p style="font-family: 'Time new roman'; padding-left: 121px;">
                                                            Tiền thuế GTGT <i>(VAT amount)</i>:
                                                        </p>
                                                    </div><div class="clsCol col-txt"><p class="dulieu">@Model.Invoice.VATAmount.ToString("N0", ViewBag.CurrentNumberFormat)</p></div>
                                                </div>
                                            </td>
                                            <td />
                                        </tr>
